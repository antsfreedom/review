.goods .fa-circle-o {
    color: #c7c7cc
}
.goods .fa-check-circle {
    color: #3399ff;
}

.page{
  /* background: #fff; */
}

.cart-list{
    position: relative;
    padding: 20rpx 20rpx 20rpx 285rpx;
    height: 185rpx;
    border-bottom: 1rpx solid #e9e9e9;
}

.cart-list:last-child{
  border-bottom:0;
}
.cart-list .cart-pro-select{
    position: absolute;
    left: 20rpx;
    top: 90rpx;
    width: 45rpx;
    height: 45rpx;
}

.cart-list .cart-thumb{
    position: absolute;
    top: 20rpx;
    left: 85rpx;
    width: 185rpx;
    height: 185rpx;
}
.cart-list .cart-pro-name{
    display: inline-block;
    height: 50rpx;
    overflow: hidden;
    font-size:22rpx;
    text-overflow: ellipsis;
    white-space: nowrap;
    width:100%;
    color:#666;
}

.cart-list .cart-pro-rule{
  font-size:22rpx;
  margin-top:-10px;
  margin-bottom:10px;
  color:#666;
}
.cart-list .cart-pro-price{
    height: 105rpx;
    line-height: 50rpx;
    margin-top:-15px;
    font-size:22rpx;
    color:#c7004a;
}
.cart-list .cart-count-box{
    position: absolute;
    left: 285;
    bottom: 0;
    width: 250rpx;
    height: 80rpx;
}
.cart-list .cart-count-box text{
  padding:2px 10px;
}
.cart-count-down,.cart-count-add{
  height:20px;
  width: 100rpx;
  border: 1px solid #999;
}
.cart-count-num{
    width: 70rpx;
    border-top:1px solid #999;
    border-bottom:1px solid #999;
}
.cart-del{
    position: absolute;
    right: 20rpx;
    bottom: 20rpx;
    width: 80rpx;
    height: 80rpx;
    line-height: 80rpx;
    text-align: center;
    font-size: 44rpx;
    color:#999;
}
.cart-top{
    position:fixed;
    left:0;
    bottom:0;
    width: 100%;
    height: 90rpx;
    line-height: 90rpx;
    padding:0 100rpx 0 80rpx;
    box-sizing: border-box;
    color: #666;
    background:#fff;
    border:1px solid #eee
}

.store-name{
  padding-top:10px;
  height:50rpx;
  line-height: 50rpx;
  padding-left:10px
}
.total-select{
    position: absolute;
    left: 20rpx;
    top: 25rpx;
    width: 45rpx;
    height: 45rpx;
}
.order-icon{
    position: absolute;
    right: 40rpx;
    top: 25rpx;
    width: 45rpx;
    height: 45rpx;
    background-size: 100%;
}
.cart-toatl-price{
    float: right;
    width: 120rpx;
}

.cart-no-data{
    padding:40rpx 0;
    color: #999;
    text-align: center;
}

.btn{
  position: absolute;
  top:0;
  right:0;
  padding:0 15px;
  color:#fff;
  background:#c7004a;
}