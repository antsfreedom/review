.page{
  background: #fff;
}

.cart-box{
    padding-bottom: 100rpx;
}
.cart-list{
    position: relative;
    padding: 20rpx 0 0 60px;
    border-bottom: 1rpx solid #e9e9e9;
}

.cart-list:last-child{
  border-bottom:0;
}
.cart-list .cart-pro-select{
    position: absolute;
    left: 20rpx;
    top: 50rpx;
    width: 45rpx;
    height: 45rpx;
}

.cart-list .cart-thumb{
    position: absolute;
    top: 20rpx;
    left: 85rpx;
    width: 185rpx;
    height: 185rpx;
}
.cart-list .cart-pro-name{
    /* display: inline-block; */
    height: 70rpx;
    overflow: hidden;
    font-size:22rpx;
    color:#666;
    padding-right:30px;
}

.cart-list .cart-pro-city{
  font-size:22rpx;
  margin-top:-20px;
  margin-bottom:10px;
  color:#666;
}

.cart-list .cart-pro-rule{
  font-size:22rpx;
  margin-top:-20px;
  margin-bottom:10px;
  color:#666;
}
.cart-list .cart-pro-price{
    /* height: 30px;
    line-height: 50rpx;
    margin-top:-15px; */
    font-size:22rpx;
    color:#666;
    margin-bottom:5px;
}

.addbtn{
  position:fixed;
  bottom:0;
  left:0;
  height:50px;
  line-height: 50px;
  width:100%;
  border:1px solid #eee;
  padding-left:20px;
  background:#c7004a;
  color:#fff;
}

.editicon{
  position: absolute;
  right:20px;
  top:30px;
}

.del{
   position: absolute;
  right:20px;
  top:60px;
}