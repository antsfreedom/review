.cover {
    width: 100px;
    height: 100px;
}
.area-picker__container {
    position: fixed;
    bottom: 0;
    left: 0;
    right: 0;
    background-color: #fff;
    box-shadow: 0px 2px -2px #e1e1e1;
    z-index: 100;
}
.area-picker__bg {
    background-color: rgba(0,0,0,0.5);
    position: absolute;
    top: 0;
    bottom: 0;
    left: 0;
    right: 0;
    z-index: 99;

}
.area-picker__header {
    background: #fbf9fe;
    line-height: 44px;
    padding-right: 30rpx;
    padding-left: 30rpx;
    color: #596993;
    font-size: 14px;
}
.area-picker-column {
    height: 44px;
    line-height: 44px;
}
.area-picker__item {
    line-height: 44px;
    margin-left: 30rpx;
    font-size: 14px;
}